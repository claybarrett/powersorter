{
  "collection": {
    "name": "BRIT",
    "prefix": "BRIT"
  },
  "files": {
    "folder_increment": 1000,
    "number_pad": 7,
    "base_output_path": "/test/out/",
    "input_path": "/test/in/"
  },
  "file_types": {
    "web_jpg": {
      "regex": "(?<prefix>BRIT)(?<numerical>\\d+)(\\.)(?i)(?<ext>jpg|jpeg)",
      "destination_path": "web/"
    },
    "web_derivs": {
      "regex": "(?<prefix>BRIT)(?<numerical>\\d+)(?<delimiter>_)(?<size>med|thumb)(\\.)(?<ext>.+)",
      "destination_path": "web/"
    },
    "archive_dng": {
      "regex": "(?<prefix>BRIT)(?<numerical>\\d+)(\\.)(?i)(?<ext>DNG)",
      "destination_path": "archive/"
    }
  }
}
