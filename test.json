{
  "collection": {
    "name": "BRIT",
    "prefix": "BRIT"
  },
  "files": {
    "folder_increment": 1000,
    "number_pad": 7,
    "base_output_path": "/test/out/",
    "input_path": "/test/in/"
  },
  "file_types": {
    "web_jpg": {
      "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(\\.)(?i)(?P<ext>jpg|jpeg)",
      "destination_path": "web/"
    },
    "web_derivs": {
      "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(?P<delimiter>_)(?P<size>med|thumb)(\\.)(?P<ext>.+)",
      "destination_path": "web/"
    },
    "archive_dng": {
      "regex": "(?P<prefix>BRIT)(?P<numerical>\\d+)(\\.)(?i)(?P<ext>DNG)",
      "destination_path": "archive/"
    }
  }
}
